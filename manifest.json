{
  "name": "无限浪采集插件",
  "description": "支持1688 ，aws 等数据采集",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  },
  "action": {},
  "content_scripts": [
    {
      "matches": [
        "https://devops.aliyun.com/organization/*"
      ],
      "css": [
        "css/content.css",
        "css/dxm_modal.css"
      ],
      "js": [
        "depends/jquery-3.6.0.min.js",
        "js/content-script.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "permissions": [
    "webRequest"
  ]
}
